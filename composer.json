{
  "name": "xylemical/code-validator",
  "type": "library",
  "license": "MIT",
  "description": "A validation framework for code object modelling.",
  "keywords": [
    "object model",
    "validation"
  ],
  "homepage": "https://github.com/xylemical/code-validator",
  "autoload": {
    "psr-4": {
      "Xylemical\\Code\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xylemical\\Code\\Validator\\": "tests/src/"
    }
  },
  "require": {
    "php": ">7.4",
    "xylemical/code": "dev-master"
  },
  "require-dev": {
    "xylemical/composer-dev-bundle": "^0.1.0"
  },
  "scripts": {
    "tests": "vendor/bin/phpunit && vendor/bin/phpstan analyse && vendor/bin/phpcs",
    "test-coverage": "php -dxdebug.mode=coverage vendor/bin/phpunit --coverage-html report"
  }
}
